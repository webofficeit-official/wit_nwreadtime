{
    "name": "wit/wit-nwreadtime",
    "type": "typo3-cms-extension",
    "description": "Wit news extension article readable time counter",
    "authors": [
        {
            "name": "Rahul R S",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11.5"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.9.0"
    },
    "autoload": {
        "psr-4": {
            "Wit\\WitNwreadtime\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wit\\WitNwreadtime\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/wit-nwreadtime": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ]
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/Web",
            "extension-key": "wit_nwreadtime"
        }
    }
}